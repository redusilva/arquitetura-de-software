generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model alunos {
  id        Int     @id @default(autoincrement())
  nome      String  @db.VarChar(255)
  email     String  @db.VarChar(255)
  matricula String  @db.VarChar(100)
  notas     notas[]
}

model disciplinas {
  id                      Int                       @id @default(autoincrement())
  nome                    String                    @db.VarChar(255)
  notas                   notas[]
  professores_disciplinas professores_disciplinas[]
}

model notas {
  id            Int         @id @default(autoincrement())
  id_aluno      Int
  id_disciplina Int
  valor         Int
  alunos        alunos      @relation(fields: [id_aluno], references: [id], onDelete: Cascade, onUpdate: Cascade, map: "fk_notas_alunos")
  disciplinas   disciplinas @relation(fields: [id_disciplina], references: [id], onDelete: Cascade, onUpdate: Cascade, map: "fk_notas_disciplinas")
}

model professores {
  id                      Int                       @id @default(autoincrement())
  nome                    String                    @db.VarChar(255)
  email                   String                    @db.VarChar(255)
  matricula               String                    @db.VarChar(100)
  professores_disciplinas professores_disciplinas[]
}

model professores_disciplinas {
  id            Int         @id @default(autoincrement())
  id_professor  Int
  id_disciplina Int
  disciplinas   disciplinas @relation(fields: [id_disciplina], references: [id], onDelete: Cascade, onUpdate: Cascade, map: "fk_disciplina")
  professores   professores @relation(fields: [id_professor], references: [id], onDelete: Cascade, onUpdate: Cascade, map: "fk_professor")
}
